<div id="<%= dom_id director %>"> 
  <tr>
    <td><%= director.name %></td>
    <td><%= format_date(director.created_at) %></td>
    <td>
      <%= link_to image_tag('icons/editIcon.png'), edit_director_path(director) %>
      <%= link_to image_tag('icons/deleteIcon.png'), director, data: { "turbo-method": :delete } %>
    </td>
  </tr>
</div>
