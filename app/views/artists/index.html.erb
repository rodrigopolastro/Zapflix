<div id="artists" class="d-flex mb-5 justify-content-between">
  <h3> Artistas </h3>
  <%= link_to "New artist", new_artist_path %>
</div>
  <p style="color: green"><%= notice %></p>
<table class="table table-striped table-dark">
  <thead>
    <tr>
      <th class="col-md-3" scope="col">Nome</th>
      <th class="col-md-3" scope="col">Registrado em</th>
      <th class="col-md-3" scope="col">Ações</th>
    </tr>
  </thead>
        <tbody>
          <% @artists.each do |artist| %>
              <%= render artist %>
          <% end %>
        </tbody>
</table>
